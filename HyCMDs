    local playerGui = player:FindFirstChild("PlayerGui")
    if playerGui then
        for _, gui in pairs(playerGui:GetChildren()) do
            if gui:IsA("GuiBase2d") and gui.Name ~= "HyCMDs" then
                gui.Enabled = false
            end
        end
    end
end,

