        antiShake.Parent = cam
        antiShake.MaxTorque = Vector3.new(4000, 4000, 4000)
        antiShake.CFrame = cam.CFrame
        antiShake.P = 10000
        antiShake.D = 5000
        antiShake.CFrame = cam.CFrame
    end
